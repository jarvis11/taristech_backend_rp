<h1>Listing mediabuyers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Password</th>
      <th>Phone</th>
      <th>Permission</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mediabuyers.each do |mediabuyer| %>
      <tr>
        <td><%= mediabuyer.first_name %></td>
        <td><%= mediabuyer.last_name %></td>
        <td><%= mediabuyer.password %></td>
        <td><%= mediabuyer.phone %></td>
        <td><%= mediabuyer.permission %></td>
        <td><%= link_to 'Show', advertiser_mediabuyers_path(@advertiser, mediabuyer) %></td>
        <td><%= link_to 'Edit', edit_advertiser_mediabuyer_path(@advertiser, mediabuyer) %></td>
        <td><%= link_to 'Destroy', [@advertiser, mediabuyer], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mediabuyer', new_advertiser_mediabuyer_path(@advertiser) %>
